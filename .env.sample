# WatchGuard
WG_AUTH_URL=https://api.usa.cloud.watchguard.com/oauth/token
WG_ACCESS_ID=read_only_access_id_is_fine
WG_ACCESS_PASSWORD=your_wg_api_password
WG_API_KEY=your_watchguard_api_key
WG_REGION=usa
WG_PARTNER_ACCOUNT_ID=ACC-your_partner_acc_number

# ConnectWise auth (same pattern you already use)
CW_CLIENT_ID=screenconnect_developer_client_id
